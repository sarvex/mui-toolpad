    "dev:cli": "tsup --watch",
    "dev:typings": "yarn build:typings",
    "waitForApp": "tsx ./scripts/waitForApp.mts",
    "check-types": "tsup && tsc --noEmit",
    "test": "jest"
  },
  "files": [
    "public",
    "scripts",
    "dist",
    "cli.js",
    ".next"
  ],
  "exports": {
