  "scripts": {
    "build": "concurrently \"yarn:build:*\" && rimraf ./.next/cache",
    "start": "next start",
    "dev": "concurrently \"yarn:dev:*\"",
    "cli": "./cli.js",
    "lint": "next lint && prettier --check .",
    "fix": "next lint --fix && prettier --write .",
    "build:cli": "tsup",
