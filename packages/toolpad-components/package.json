{
  "name": "@mui/toolpad-components",
  "version": "0.1.19",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-toolpad.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "dependencies": {
    "@mui/icons-material": "5.13.7",
    "@mui/lab": "5.0.0-alpha.135",
    "@mui/material": "5.13.7",
    "@mui/toolpad-core": "0.1.19",
    "@mui/toolpad-utils": "0.1.19",
    "@mui/x-data-grid-pro": "7.3.1",
    "@mui/x-date-pickers": "7.3.1",
    "dayjs": "1.11.9",
    "lodash-es": "4.17.21",
    "markdown-to-jsx": "7.2.1",
    "react-error-boundary": "4.0.10",
    "react-hook-form": "7.45.1"
  },
  "devDependencies": {
    "react": "18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "08fb1bad6e60086d1b775f377101b88b3d1a9e5d"
}
